---
import feature from "../../config/fature.json";
---

<h1 class="text-center text-5xl mt-10">Feature</h1>

<div class="grid md:grid-cols-3 gap-4 p-4">
  {
    feature.map((data) => (
      <div class="bg-slate-800 p-10 rounded-x1">
        <h2 class="text-2xl my-4 ">{data.title}</h2>
        <p>{data.description}.</p>
      </div>
    ))
  }
</div>
